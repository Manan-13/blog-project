<%- include('partials/header'); -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="mb-3">
      <label for="exampleInputEmail1" class="form-label">Title</label>
      <input name="postTitle" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    </div>
    <br>
    <div class="mb-3">
      <label for="exampleInputPassword1" class="form-label">Post</label>
      <textarea name="postBody" class="form-control" id="exampleInputPassword1" rows="4" cols="50"> </textarea>
    </div>
    <br>
    <button type="submit" class="btn btn-primary">Publish</button>
  </form>

<%- include('partials/footer'); -%>